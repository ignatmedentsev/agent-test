{
  "name": "marketplace-agent",
  "version": "1.0.0",
  "main": "main/main.js",
  "author": {
    "name": "marketplace.nanox",
    "url": "https://marketplace.nanox.vision/",
    "email": "support@mdw.io"
  },
  "homepage": "https://mdw.io/",
  "repository": "https://github.com/MDWio/marketplace-agent-release",
  "description": "Desktop client for Nanox Marketplace",
  "scripts": {
    "render:start": "ng serve",
    "render:build": "ng build --base-href ./",
    "render:watch": "ng build --base-href ./ --watch --configuration development",
    "render:clear": "rimraf dist/render",
    "render:lint": "ng lint",
    "render:lint:fix": "npm run render:lint -- --fix",
    "main:build": "nest build -p \"./src/main/tsconfig.main.json\"",
    "main:start": "npx electron \"./dist/main/main.js\"",
    "main:up": "npm run main:build && npm run main:start",
    "main:dev": "cross-env NODE_ENV=dev nodemon --config \"./nodemon.json\" --exec npm run main:up",
    "main:lint": "eslint --config \"./.eslintrc.json\" \"./src/main/**/*.ts\" \"./src/common/**/*.ts\"",
    "main:lint:fix": "npm run main:lint -- --fix",
    "style:lint": "stylelint --config \"./.stylelintrc.json\" \"./src/render/app/**/*.(c|sa|sc)ss\"",
    "style:lint:fix": "npm run style:lint -- --fix",
    "rm:release": "rimraf release",
    "lint:all": "npm run render:lint && npm run style:lint && npm run main:lint",
    "lint:all:fix": "npm run render:lint:fix && npm run style:lint:fix && npm run main:lint:fix",
    "asar:extract:linux": "npx asar extract \"./release/linux-unpacked/resources/app.asar\" \"./asar-src\"",
    "asar:extract:win": "npx asar extract \"./release/win-unpacked/resources/app.asar\" \"./asar-src\"",
    "electron:builder": "npm run rm:release && npx electron-builder build",
    "electron:build:src": "npm run render:build && npm run main:build",
    "electron:linux": "npm run electron:build:src && npm run electron:builder -- --linux",
    "electron:windows": "npm run electron:build:src && npm run electron:builder -- --windows"
  },
  "private": true,
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.4",
    "@nestjs/platform-express": "^9.1.4",
    "axios": "^1.1.2",
    "cross-env": "^7.0.3",
    "electron-updater": "^5.2.1",
    "module-alias": "^2.2.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0",
    "winston": "^3.8.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular-eslint/builder": "^14.1.2",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular-eslint/eslint-plugin-template": "^14.1.2",
    "@angular-eslint/schematics": "^14.1.2",
    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.3",
    "@angular/cli": "~14.2.3",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/compiler-cli": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.3",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@types/express": "^4.17.14",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^29.0.3",
    "@types/mocha": "^9.1.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.7.23",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "cpy-cli": "^4.2.0",
    "electron": "^20.2.0",
    "electron-builder": "^23.3.3",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "nodemon": "^2.0.20",
    "postcss-scss": "^4.0.5",
    "rimraf": "^3.0.2",
    "stylelint": "^14.12.1",
    "tslib": "^2.4.0",
    "typescript": "~4.7.2"
  },
  "engines": {
    "node": "16.*",
    "npm": "8.*"
  }
}
